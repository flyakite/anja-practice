#lsc -cj package.ls
author: 'Shih-Wen Su'
name: 'anja'
description: 'anja'
version: '0.1.1'
homepage: 'https://github.com/flyakite/anja'
repository:
  type: 'git'
  url: 'https://github.com/flyakite/anja'  
engines:
  node: '~0.6.10 || 0.8 || 0.9'
  npm: '1.2.x'
scripts:
  start: 'node_modules/.bin/brunch watch --server'
  test: 'node_modules/.bin/karma start test/karma.conf.js'
dependencies:
  brunch: '1.5.x'
  bower: '~0.9.2'
  LiveScript: '>=1.1.1'
  'coffee-script': '>= 1.4'
  'javascript-brunch': '1.5.x'
  'coffee-script-brunch': '1.5.x'
  'Livescript-brunch': '1.5.x'
  'less-brunch': '1.5.x'
  'css-brunch': '1.5.x'
  'sass-brunch': '1.5.x'
  'stylus-brunch': '1.5.x'
  jade: '>= 0.28'
  'jade-brunch': '1.4.x'
  'static-jade-brunch': '>= 1.4.0 <= 1.4.5 || >= 1.4.8 < 1.5'
  'jade-angularjs-brunch': '>= 0.0.5 <= 0.1',
  'auto-reload-brunch': '1.5.x'
  'uglify-js-brunch': '1.5.x'
  'clean-css-brunch': '1.5.x'
  'bower-stylesheet-brunch': 'git://github.com/angular-brunch/bower-stylesheet-brunch/#master'
  'bower-javascript-brunch': 'git://github.com/angular-brunch/bower-javascript-brunch/#master'
  'bower-asserts-brunch': 'git://github.com/angular-brunch/bower-assets-brunch/#master'
  karma: '>= 0.8.4 < 0.9.0'
